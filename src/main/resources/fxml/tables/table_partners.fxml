<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.control.*?>
<TableView xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:id="table" fx:controller="com.futurteam.dbmusic.controllers.tables.PartnersTableController">
  <columnResizePolicy>
    <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
  </columnResizePolicy>
  <columns>
    <TableColumn editable="false" minWidth="-1.0" prefWidth="75.0" text="id">
      <cellValueFactory>
        <PropertyValueFactory property="id"/>
      </cellValueFactory>
    </TableColumn>
    <TableColumn editable="false" minWidth="-1.0" prefWidth="75.0" text="Наименование">
      <cellValueFactory>
        <PropertyValueFactory property="name"/>
      </cellValueFactory>
    </TableColumn>
    <TableColumn editable="false" minWidth="-1.0" prefWidth="75.0" text="Стоимость размещения">
      <cellValueFactory>
        <PropertyValueFactory property="price"/>
      </cellValueFactory>
    </TableColumn>
  </columns>
</TableView>